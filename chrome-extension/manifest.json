{
  "manifest_version": 3,
  "name": "Cursor AI Element Builder for Webflow",
  "version": "1.0.0",
  "description": "Add AI-generated elements to your Webflow pages using natural language prompts",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://webflow.com/*",
    "https://*.webflow.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://webflow.com/design/*",
        "https://*.webflow.com/design/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Cursor AI Element Builder",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "panel.html",
        "panel.js",
        "panel.css",
        "icons/*"
      ],
      "matches": [
        "https://webflow.com/*",
        "https://*.webflow.com/*"
      ]
    }
  ]
}